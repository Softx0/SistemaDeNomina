@model SISTEMANOMINA.TIPO_DE_DEDUCCION

@{
    ViewBag.Title = "Eliminar";
}

<h2>Eliminar</h2>

<h3>Estas seguro de eliminar este registro?</h3>
<div>
    <h4>Tipo de Deduccion</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.Label("Deduccion")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NOMBRE_TIPO_DEDUCCION)
        </dd>

        <dt>
            @Html.Label("Porcentaje")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PORCENTAJE)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-danger" /> |
            @Html.ActionLink("Volver Atras", "Index")
        </div>
    }
</div>
